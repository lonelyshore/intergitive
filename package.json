{
  "name": "intergitive",
  "version": "0.0.1",
  "description": "An interactive git tutorial",
  "main": "main.js",
  "dependencies": {
    "fs-extra": "^8.0.0",
    "js-yaml": "^3.13.1",
    "nodegit": "^0.24.3",
    "vue": "^2.6.10"
  },
  "devDependencies": {
    "adm-zip": "^0.4.13",
    "chai": "^4.2.0",
    "chai-as-promised": "^7.1.1",
    "electron": "^4.2.2",
    "electron-mocha": "^8.0.2",
    "electron-packager": "^13.1.1",
    "electron-rebuild": "^1.8.5",
    "mocha": "^6.1.4",
    "seedrandom": "^3.0.1",
    "simple-git": "^1.110.0"
  },
  "scripts": {
    "test": "mocha --recursive \"./test/tests/*.js\"",
    "test-core": "mocha --recursive \"./test/tests/*.js\" --grep \"#core\"",
    "start": "electron .",
    "rebuild": "electron-rebuild -f --version 4.2.2 -w nodegit",
    "etest": "electron-mocha --renderer --recursive \"./test/tests/*.js\"",
    "etest-core": "electron-mocha --renderer --recursive \"./test/tests/*.js\" --grep \"#core\"",
    "build-win64": "electron-packager . intergitive --platform=win32 --arch=x64",
    "build-win64-dev": "electron-packager . intergitive --platform=win32 --arch=x64 --prune=false"
  },
  "author": "zedaizd",
  "license": "ISC"
}
